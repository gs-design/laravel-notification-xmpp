{
    "name": "gs-design/laravel-notification-xmpp",
    "description": "XMPP Notifications Drivers for Laravel",
    "keywords": [
        "xmpp notification",
        "laravel",
        "xmpp",
        "notification",
        "xmpp notifications channel"
    ],
    "homepage": "https://github.com/gs-design/laravel-notification-xmpp",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr H.",
            "email": "gs.design.git@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "tyrola/xmpphp": "^2.2",
        "illuminate/notifications": "^5.5 || ^6.0",
        "illuminate/support": "^5.5 || ^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "GsDesign\\XMPP\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GsDesign\\XMPP\\XMPPServiceProvider"
            ]
        }
    }
}
